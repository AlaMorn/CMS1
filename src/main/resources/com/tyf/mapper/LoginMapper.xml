<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tyf.mapper.LoginMapper">
    <select id="selectUserByUsernameAndPassword" parameterType="com.tyf.pojo.login" resultType="com.tyf.pojo.login">
        select * from login where id=#{id} and password=#{password} and power=#{power}
    </select>
</mapper>